{
    "version": 2,
    "builds": [
        {
            "src": "dbtest.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "10mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dbtest.py"
        }
    ]
}
